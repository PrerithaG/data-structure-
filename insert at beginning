#include<stdio.h>
#define cap 10
int main()
{
    int a[cap] = {1,4,2,6,8,40};
    int n = 3;
    int x = 6;
    if(n<cap)
    {
        for(int i =n ; i>0 ; i--)
         a[i]=a[i-1];
       a[x]=x;
       n++;
    }
    else{
    printf("nadi manege");
    }
    for(int i = 0 ; i<n ; i++)
    printf("%d ",a[i]);
}
