//fixed capacity by using macro function called CAP
#include<stdio.h>
#define CAP 10
int main()
{
    int a[5]={1,2,3,4,5},n=5;
    void printArray(int a[5],int n)
    {
        printf("[");
        for(int i=0;i<n&&i<CAP;i++)
        printf("%d ",a[i]);
        printf("]\n");
    }
    printArray(a,5);
    }
